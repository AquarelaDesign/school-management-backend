{
  "name": "school-management-backend",
  "version": "0.1.0",
  "description": "This is a school management system built using MERN stack",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "babel-node server",
    "server": "nodemon server",
    "client": "npm run dev --prefix frontend",
    "dev": "npm run server",
    "data:import": "node seeder",
    "data:destroy": "node seeder -d",
    "cluster-start": "npm i && pm2 start ./pm2-ws.json",
    "cluster-stop": "pm2 delete libws-1 && pm2 delete libws-2 && pm2 delete libws-3 && pm2 delete libws-4",
    "background": "nodemon backgroundServices/index",
    "test": "jest --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AquarelaDesign/school-management-backend.git"
  },
  "author": "Jose Augusto Kreminski Lustosa Freire",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AquarelaDesign/school-management-backend/issues"
  },
  "homepage": "https://github.com/AquarelaDesign/school-management-backend#readme",
  "dependencies": {
    "axios": "^1.6.5",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^8.6.0",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-async-handler": "^1.1.4",
    "handlebars": "^4.7.7",
    "html-to-text": "^9.0.5",
    "imap": "^0.8.19",
    "imap-simple": "^5.1.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb-memory-server": "^9.1.3",
    "mongoose": "^6.8.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "ngrok": "^5.0.0-beta.2",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.13",
    "react-icons": "^4.10.1",
    "request": "^2.88.2",
    "useragent": "^2.3.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/node": "^7.22.19",
    "@babel/preset-env": "^7.23.7",
    "concurrently": "^5.3.0",
    "jest": "^29.7.0",
    "nodemon": "^2.0.4"
  }
}
